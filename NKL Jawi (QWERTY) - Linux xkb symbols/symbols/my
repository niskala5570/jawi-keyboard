xkb_symbols "jawiniskala"
{
    name[Group1] = "Malay (Jawi NISKALA, phonetic)";


    key <TLDE>	{[   grave,	 asciitilde,  U223D   ]};
    key <AE01>	{[	 1,	 exclam,  U0661   ]};
    key <AE02>	{[	 2,	 at,  U0662   ]};
    key <AE03>	{[	 3,	 numbersign,  U0663   ]};
    key <AE04>	{[	 4,	 dollar,  U0664   ]};
    key <AE05>	{[	 5,	 percent,  U0665   ]};
    key <AE06>	{[	 6,	 asciicircum, U0666   ]};
    key <AE07>	{[	 7,	 ampersand,  U0667   ]};
    key <AE08>	{[	 8,	 asterisk,  U0668   ]};
    key <AE09>	{[	 9,	 parenleft,  U0669   ]};
    key <AE10>	{[	 0,	 parenright,  U0660    ]};
    key <AE11>	{[   minus,	 underscore, Arabic_tatweel   ]};
