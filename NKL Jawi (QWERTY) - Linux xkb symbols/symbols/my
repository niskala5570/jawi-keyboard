    key <AE06> {[  U0666,      asciicircum,  6  ]};
    key <AE07> {[  U0667,        ampersand,  7  ]};
    key <AE08> {[  U0668,         asterisk,  8  ]};
    key <AE09> {[  U0669,       parenright,  9  ]};
    key <AE10> {[  U0660,        parenleft,  0  ]};
