    key <AD11> {[bracketright,  braceright,  U2067, U200F]};
    key <AD12> {[ bracketleft,  braceleft,  U2069,  U200E]};

    key <AC01> {[ Arabic_alef,  Arabic_hamza,  U0654,  U06DD   ]};
